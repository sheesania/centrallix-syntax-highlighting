{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Centrallix",
	"patterns": [
		{
			"include": "#variable-keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"variable-keywords": {
			"name": "entity.other.attribute-name.centrallix",
			"match": "\\b(width|height)\\b"
		},
		"strings": {
			"patterns": [
				{
					"name": "string.quoted.double.centrallix",
					"begin": "\"",
					"end": "\""
				},
				{
					"name": "string.quoted.single.centrallix",
					"begin": "'",
					"end": "'"
				}
			]
		}
	},
	"scopeName": "source.centrallix"
}